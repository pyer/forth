gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/term-sub.o src/term-sub.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/toolbelt-ext.o src/toolbelt-ext.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/tools-ext.o src/tools-ext.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/tools-mix.o src/tools-mix.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/tools-sub.o src/tools-sub.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/useful-ext.o src/useful-ext.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/version-sub.o src/version-sub.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/with-spy.o src/with-spy.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/your-ext.o src/your-ext.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/zchar-ext.o src/zchar-ext.c

gcc -DARM -g -Wall -Wstrict-prototypes -W,--warn-common -fPIC -rdynamic -o pf target/main.o target/core-sub.o target/core-ext.o target/core-mix.o target/cdecl-ext.o target/dict-sub.o target/dict-comp.o target/header-sub.o target/header-ext.o target/double-sub.o target/double-ext.o target/double-mix.o target/exception-sub.o target/exception-ext.o target/engine-sub.o target/engine-ext.o target/engine-set.o target/environ-ext.o target/facility-ext.o target/facility-mix.o target/file-ext.o target/file-sub.o target/file-mix.o target/floating-ext.o target/floating-mix.o target/locals-ext.o target/memory-alloc-ext.o target/tools-sub.o target/tools-ext.o target/tools-mix.o target/search-order-ext.o target/string-ext.o target/memory-sub.o target/debug-ext.o target/lined.o target/forth-83-ext.o target/forth-usual-ext.o target/misc-ext.o target/posix-ext.o target/shell-os-ext.o target/signals-ext.o target/system-ext.o target/useful-ext.o target/your-ext.o target/term-sub.o target/term-ext.o target/term-lib.o target/toolbelt-ext.o target/with-spy.o target/version-sub.o target/_missing.o target/os-string.o target/os-ctype.o target/os-delay.o target/p4-gettimeofday.o target/zchar-ext.o -lm
