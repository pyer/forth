gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/misc-stk.o src/misc-stk.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/_missing.o src/_missing.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/module-ext.o src/module-ext.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/os-ctype.o src/os-ctype.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/os-delay.o src/os-delay.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/os-string.o src/os-string.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/p4-gettimeofday.o src/p4-gettimeofday.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/posix-ext.o src/posix-ext.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/search-order-ext.o src/search-order-ext.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/shell-os-ext.o src/shell-os-ext.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/signals-ext.o src/signals-ext.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/socket-ext.o src/socket-ext.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/string-ext.o src/string-ext.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/string-stk.o src/string-stk.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/struct-ext.o src/struct-ext.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/structs-ext.o src/structs-ext.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/system-ext.o src/system-ext.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/term-ext.o src/term-ext.c
