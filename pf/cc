gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/engine-sub.o src/engine-sub.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/environ-ext.o src/environ-ext.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/exception-ext.o src/exception-ext.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/exception-stk.o src/exception-stk.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/exception-sub.o src/exception-sub.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/facility-ext.o src/facility-ext.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/facility-mix.o src/facility-mix.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/facility-stk.o src/facility-stk.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/file-ext.o src/file-ext.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/file-mix.o src/file-mix.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/file-stk.o src/file-stk.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/file-sub.o src/file-sub.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/floating-ext.o src/floating-ext.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/floating-mix.o src/floating-mix.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/forth-83-ext.o src/forth-83-ext.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/forth-83-stk.o src/forth-83-stk.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/forth-usual-ext.o src/forth-usual-ext.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/fpnostack-ext.o src/fpnostack-ext.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/header-ext.o src/header-ext.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/header-sub.o src/header-sub.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/lined.o src/lined.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/locals-ext.o src/locals-ext.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/main.o src/main.c
