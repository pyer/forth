#!/bin/sh

ARCH="arm"
mkdir target

gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/cdecl-ext.o src/cdecl-ext.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/cdecl-stk.o src/cdecl-stk.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/complex-ext.o src/complex-ext.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/core-ext.o src/core-ext.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/core-mix.o src/core-mix.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/core-stk.o src/core-stk.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/core-sub.o src/core-sub.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/debug-ext.o src/debug-ext.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/debug-stk.o src/debug-stk.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/dict-comp.o src/dict-comp.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/dict-sub.o src/dict-sub.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/double-ext.o src/double-ext.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/double-mix.o src/double-mix.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/double-sub.o src/double-sub.c
gcc -DARM -Isrc -Isrc/include -Isrc/pfe -O1 -g -Wall -fPIC -Wstrict-prototypes -c -o target/dstrings-ext.o src/dstrings-ext.c
